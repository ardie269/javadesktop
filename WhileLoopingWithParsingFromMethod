package learningjava;

import java.io.BufferedReader;
import java.util.List;

public class Basic1 {
    boolean use ;
    public static void main(String[]args){
        int number = 1;
        while(number <6){
            System.out.print(number+" ");
            number += 1;
        } // output : 1 2 3 4 5 
        Basic1 call = new Basic1();
        call.ArrayFor();
    }
    
    public void ArrayFor(){
        System.out.println(use);
    }
}

/*
while looping with parsing data from method 
Output : 
1 2 3 4 5 false
*/
